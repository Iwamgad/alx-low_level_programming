#include "main.h"

/**
 * main - Entry point
 * print_alphabet_x10 - prints alphabets in lowercase ten times
 * Return: Always 0 (Success)
 */
void print_alphabet_x10(void)
{
char ch;
int i = 1;
while (i <= 10)
{
for (ch = 'a'; ch <= 'z'; ch++)
{
_putchar(ch);
}
if (i != 10)
_putchar('\n');
i++;
}
}
